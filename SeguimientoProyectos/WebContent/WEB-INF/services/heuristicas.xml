			<viewComponent>

				<form columns="3" title="Consulta heurísticas">

					<fieldViewSet entitymodel="configuradorEstudios"
						nameSpace="configuradorEstudios">
						<fieldView mappingTo="2"/>						
					</fieldViewSet>
				</form>

				<grid title="Listado heurísticas" orderDirection="asc" eventsRegistered="update, create, delete">
					<fieldViewSet entitymodel="configuradorEstudios"
						nameSpace="configuradorEstudios">
						<fieldView mappingTo="1" hidden="true"/>
						<fieldView mappingTo="2" />
					</fieldViewSet>
				</grid>

			</viewComponent>
		</action>

