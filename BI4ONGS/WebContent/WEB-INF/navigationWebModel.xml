<?xml version="1.0" encoding="UTF-8"?>

<app:application xmlns:app="http://pcm.framework.org/metamodel"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://pcm.framework.org/metamodel navigationwebmodel.xsd">
	
	<profiles>
		<profile name="ADMINISTRADOR" description="Usuario administrador" />
		<profile name="ASOCIADO" description="Asociación gestora" />
		<profile name="ONG_TEAM" description="Miembro de ONG" />
	</profiles>

	<logo content="img/logoPCM.PNG" width="202" height="72" alt="Inicio" uri="bimanager"/>

	<foot text="Business Intelligence para organizaciones de carácter no lucrativo v&lt;b&gt;&lt;i&gt;01.00&lt;/i&gt;&lt;/b&gt; [05/04/2021]" link="bimanager" />

	<tree profile="ADMINISTRADOR, ASOCIADO" name="tree">

		<FOLDER profile="ADMINISTRADOR, ASOCIADO" name="Entidades colaborativas">
			<LEAF link="event=GestionEntidades.query#entityName=unidadOrg" name="Organismos" />
			<LEAF link="event=GestionEntidades.query#entityName=subdireccion" name="Subdirecciones" />
		</FOLDER>
		
		<FOLDER profile="ADMINISTRADOR, ASOCIADO" name="Seguimiento CD">
			<LEAF link="event=GestionTech.query" name="Tecnologías" />
			<LEAF link="event=GestionServiciosUTE.query" name="Servicios UTE" />
			<LEAF link="event=GestionApps.query" name="Aplicativos" />
			<LEAF link="event=ConsultaPeticionesGEDEON.query" name="GEDEON(es)" />
			<LEAF link="event=EstudioPeticiones.query" name="Estudios Peticiones" />
			<LEAF link="event=GestionHeuristicas.query" name="Heurísticas aplicadas" />		
			<FOLDER profile="ADMINISTRADOR, ASOCIADO"	name="Importaciones Gedeon/Artemis">
				<LEAF link="event=GestionarImportacionesGEDEON.query" name="Gedeones" />
				<LEAF link="event=GestionarImportacionesARTEMIS.query" name="Artemis" />
			</FOLDER>
			<FOLDER profile="ADMINISTRADOR, ASOCIADO" name="Demos">
				<LEAF link="event=Demos.query" name="Gráficos - mapas" />
			</FOLDER>
		</FOLDER>
				
		<FOLDER profile="ADMINISTRADOR" name="Site technical config.">
			<LEAF link="exec=Configuration" name="Configuraciones" />
			<LEAF link="event=GestionEntidades.query#entityName=rol" name="Roles" />
			<LEAF link="event=GestionUsuarios.query" name="Usuarios" />
			<LEAF link="event=GestionEntidades.query#entityName=tipoAplicacion" name="Technologies" />
			<LEAF link="event=GestionEntidades.query#entityName=mes" name="Meses" />
			<LEAF link="event=GestionEntidades.query#entityName=tiposPeticiones" name="Tipos Peticiones" />
		</FOLDER>
				
	</tree>	
	
</app:application>
